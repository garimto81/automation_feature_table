# PokerGFX Configuration
POKERGFX_API_URL=ws://localhost:8080
POKERGFX_API_KEY=your_pokergfx_api_key

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.5-flash-native-audio-preview

# Video Capture Configuration
VIDEO_STREAMS=rtsp://table1:554/stream,rtsp://table2:554/stream
VIDEO_FPS=1

# Database Configuration (PostgreSQL)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=poker_hands
DB_USER=postgres
DB_PASSWORD=your_db_password

# vMix Configuration
VMIX_HOST=127.0.0.1
VMIX_PORT=8088
VMIX_AUTO_RECORD=true

# Recording Configuration
RECORDING_PATH=./recordings

# Fallback Configuration
FALLBACK_ENABLED=true

# Logging
LOG_LEVEL=INFO
